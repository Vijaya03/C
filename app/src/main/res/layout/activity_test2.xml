<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.vijaya.c.test2"
    android:background="#fff"
   >

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:src="@drawable/card"
            />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/log"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/friendrequest"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/notification"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="10dp"
            />
        <ImageButton
            android:layout_width="5dp"
            android:layout_height="22dp"
            android:background="@drawable/menu2"
            android:layout_marginLeft="350dp"
            android:layout_marginTop="10dp"
            android:layout_alignParentEnd="true"
            android:id="@+id/imageButton5" />
    </FrameLayout>
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"

    >
    <Button
        android:layout_width="175dp"
        android:layout_height="45dp"
        android:layout_marginLeft="18dp"
        android:textColor="#fff"
        android:text="My Suggestions"
        android:textAllCaps="false"
        android:background="@drawable/card"/>
    <Button
        android:layout_width="175dp"
        android:layout_height="45dp"
        android:layout_marginLeft="0dp"
        android:background="@drawable/card"
        android:text="My Connections"/>
</LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="17dp"
        android:background="@drawable/layout"
        android:layout_marginBottom="10dp">

        <SearchView
            android:layout_width="267dp"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="65dp"
            android:layout_marginTop="27dp"
            android:background="@drawable/search"
            android:weightSum="1"
            android:id="@+id/searchView"
            android:queryHint="Batminton"
            ></SearchView>

     <ImageView
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:src="@drawable/searchh"
         android:layout_marginTop="30dp"
         android:layout_marginLeft="280dp"
       />
       <GridLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:columnCount="2"
           android:rowCount="2">
           <android.support.v7.widget.CardView
               android:layout_height="140dp"
               android:layout_width="140dp"
               android:layout_marginBottom="16dp"
               android:layout_marginLeft="16dp"
               android:layout_marginRight="16dp"
               android:layout_marginTop="70dp"
               app:cardBackgroundColor="#fff"
               >
               <RelativeLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/boy"
                       android:layout_marginLeft="42dp"
                       android:layout_marginTop="10dp"

                       />
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/fb"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="80dp"/>

                   <TextView
                       android:id="@+id/textView7"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Viswadeep"
                       android:layout_marginTop="80dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/insta"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="100dp"/>

                   <TextView
                       android:id="@+id/textView9"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Badminton"
                       android:layout_marginTop="100dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>
                   <Button
                       android:layout_width="match_parent"
                       android:layout_height="27dp"
                       android:background="@drawable/card"
                       android:text="Add"
                       android:textAllCaps="false"
                       android:layout_marginTop="120dp"/>


               </RelativeLayout>
           </android.support.v7.widget.CardView>

           <android.support.v7.widget.CardView
               android:layout_height="140dp"
               android:layout_width="140dp"
               android:layout_marginBottom="16dp"
               android:layout_marginLeft="16dp"
               android:layout_marginRight="16dp"
               android:layout_marginTop="70dp"
               app:cardBackgroundColor="#fff"
               >
               <RelativeLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/girl"
                       android:layout_marginLeft="42dp"
                       android:layout_marginTop="10dp"

                       />
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/fb"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="80dp"/>

                   <TextView
                       android:id="@+id/textView10"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Sheetal"
                       android:layout_marginTop="80dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/insta"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="100dp"/>

                   <TextView
                       android:id="@+id/textView11"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Badminton"
                       android:layout_marginTop="100dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>
                   <Button
                       android:layout_width="match_parent"
                       android:layout_height="27dp"
                       android:background="@drawable/card"
                       android:text="Add"
                       android:textAllCaps="false"
                       android:layout_marginTop="120dp"/>


               </RelativeLayout>
           </android.support.v7.widget.CardView>

           <android.support.v7.widget.CardView
               android:layout_height="140dp"
               android:layout_width="140dp"
               android:layout_marginBottom="16dp"
               android:layout_marginLeft="16dp"
               android:layout_marginRight="16dp"
               app:cardBackgroundColor="#fff"

               >
               <RelativeLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/girl2"
                       android:layout_marginLeft="42dp"
                       android:layout_marginTop="10dp"

                       />
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/fb"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="80dp"/>

                   <TextView
                       android:id="@+id/textView12"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Mahika"
                       android:layout_marginTop="80dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/insta"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="100dp"/>

                   <TextView
                       android:id="@+id/textView13"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Badminton"
                       android:layout_marginTop="100dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>
                   <Button
                       android:layout_width="match_parent"
                       android:layout_height="27dp"
                       android:background="@drawable/card"
                       android:text="Add"
                       android:textAllCaps="false"
                       android:layout_marginTop="120dp"/>


               </RelativeLayout>
           </android.support.v7.widget.CardView>

           <android.support.v7.widget.CardView
               android:layout_height="140dp"
               android:layout_width="140dp"
               android:layout_marginBottom="16dp"
               android:layout_marginLeft="16dp"
               android:layout_marginRight="16dp"
              app:cardBackgroundColor="#fff">
               <RelativeLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/man"
                       android:layout_marginLeft="42dp"
                       android:layout_marginTop="10dp"

                       />
                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/fb"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="80dp"/>

                   <TextView
                       android:id="@+id/textView14"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Pawan"
                       android:layout_marginTop="80dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/insta"
                       android:layout_marginLeft="10dp"
                       android:layout_marginTop="100dp"/>

                   <TextView
                       android:id="@+id/textView15"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:text="Badminton"
                       android:layout_marginTop="100dp"
                       android:layout_marginLeft="40dp"
                       android:textColor="#000"/>
                   <Button
                       android:layout_width="match_parent"
                       android:layout_height="27dp"
                       android:background="@drawable/card"
                       android:text="Add"
                       android:textAllCaps="false"
                       android:layout_marginTop="120dp"/>


               </RelativeLayout>
           </android.support.v7.widget.CardView>
       </GridLayout>
    </RelativeLayout>


    <android.support.design.widget.BottomNavigationView
        android:id="@+id/navigation"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_gravity="bottom"
        android:background="#F5A623"
        app:menu="@menu/navigation"
        />

</LinearLayout>
